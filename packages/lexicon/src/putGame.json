				}
			},
			"output": {
				"encoding": "application/json",
				"schema": {
					"type": "object",
					"required": ["game"],
					"properties": {
						"game": {
							"type": "ref",
							"ref": "games.gamesgamesgamesgames.defs#gameDetailView"
						}
					}
				}
			}
		}
	}
